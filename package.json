{
  "name": "morning-streching",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "biome:format:check": "pnpm exec biome format",
    "biome:format:fix": "pnpm exec biome format --write --no-errors-on-unmatched",
    "biome:lint:check": "pnpm exec biome lint",
    "biome:lint:fix": "pnpm exec biome lint --write --no-errors-on-unmatched"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "npm-run-all2": "^8.0.4",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vue-tsc": "^3.1.1"
  }
}
